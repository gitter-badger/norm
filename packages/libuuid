#!/bin/bash


fetch_source https://www.kernel.org/pub/linux/utils/util-linux/v2.25/util-linux-2.25.2.tar.xz 854f37255ca8098e88fb4220c244b727f5dd6d58

do_unpack
pushd_src
do_configure --disable-all-programs --enable-libuuid --without-ncurses --without-python --with-bashcompletiondir=$PREFIX/share/bash-completion/completions
do_make
do_install
popd

