#!/bin/bash

depends_on mesa
## mesa listed first because mesa has gcc version check and we can bail out early


fetch_source http://http.debian.net/debian/pool/main/f/freeglut/freeglut_2.8.1.orig.tar.gz 7330b622481e2226c0c9f6d2e72febe96b03f9c4
fetch_debian http://http.debian.net/debian/pool/main/f/freeglut/freeglut_2.8.1-2.debian.tar.xz 3bb4808f0915138950d14ab609c432b8a7d8eca3

do_undebian
do_compile
