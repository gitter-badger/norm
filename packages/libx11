#!/bin/bash

depends_on autoconf automake libtool
depends_on xproto
depends_on libxcb
depends_on xtrans
depends_on xorg-macros

fetch_source http://http.debian.net/debian/pool/main/libx/libx11/libx11_1.6.2.orig.tar.gz 351ae5bad88bb2b54b7f749f6096b518a3b13b29
fetch_debian http://http.debian.net/debian/pool/main/libx/libx11/libx11_1.6.2-3.diff.gz 211933106ee61e44c98b1bed2767f10c1dc98b97
do_undebian
do_autoreconf
do_compile --disable-selective-werror
