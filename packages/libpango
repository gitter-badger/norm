#!/bin/bash

depends_on libcairo

fetch_source http://ftp.de.debian.org/debian/pool/main/p/pango1.0/pango1.0_1.30.0.orig.tar.xz d49c1ef1cbbe326b5d70e32d84f7163e7064c77c
fetch_debian http://ftp.de.debian.org/debian/pool/main/p/pango1.0/pango1.0_1.30.0-1.debian.tar.xz e1b72f8e1985da071f571aca6b80884860cf72b7
do_undebian
do_compile
